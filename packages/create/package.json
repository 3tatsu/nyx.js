{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nyxjs/create",
    "version": "1.0.0",
    "description": "Create a new Nyx.js project",
    "bin": "./dist/index.js",
    "license": "AGPL-3.0-only",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/index.js"
        }
    },
    "engines": {
        "node": ">=20"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/3tatsu/nyx.js",
        "directory": "packages/create"
    },
    "bugs": {
        "url": "https://github.com/3tatsu/nyx.js/issues"
    },
    "author": {
        "name": "3tatsu",
        "url": "https://github.com/3tatsu"
    },
    "scripts": {
        "build": "tsup && rimraf .tsup",
        "clean": "rimraf dist",
        "format": "biome format --write ."
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^12.1.0",
        "fs-extra": "^11.2.0",
        "glob": "^11.0.0",
        "inquirer": "^12.0.1",
        "ora": "^8.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@microsoft/api-extractor": "^7.47.11",
        "@swc/core": "^1.7.40",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^22.8.4",
        "rimraf": "^6.0.1",
        "terser": "^5.36.0",
        "tsup": "^8.3.5",
        "typescript": "^5.6.3"
    },
    "files": ["dist", "templates"],
    "readme": "README.md"
}
