{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxjs/store",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "tsup && rimraf .tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --fix",
    "pretty": "prettier --write src",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.9",
    "@swc/core": "^1.7.26",
    "@types/node": "^22.5.5",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "terser": "^5.33.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}