{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxjs/gateway",
  "description": "A gateway for the Nyx.js framework",
  "version": "1.0.0",
  "type": "commonjs",
  "readme": "README.md",
  "license": "AGPL-3.0-only",
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/esm/index.js",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "3tatsu",
    "url": "https://github.com/3tatsu"
  },
  "repository": {
    "type": "git",
    "directory": "packages/gateway",
    "url": "https://github.com/3tatsu/nyx.js"
  },
  "bugs": {
    "url": "https://github.com/3tatsu/nyx.js/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup && rimraf .tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --fix --format pretty",
    "pretty": "prettier --write src"
  },
  "dependencies": {
    "@nyxjs/core": "workspace:*",
    "@nyxjs/rest": "workspace:*",
    "@nyxjs/store": "workspace:*",
    "bufferutil": "^4.0.8",
    "erlpack": "^0.1.4",
    "eventemitter3": "^5.0.1",
    "utf-8-validate": "^6.0.4",
    "ws": "^8.18.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.9",
    "@swc/core": "^1.7.28",
    "@types/node": "^22.7.4",
    "@types/ws": "^8.5.12",
    "eslint": "^8.57.1",
    "eslint-formatter-pretty": "^5.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "terser": "^5.34.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist"
  ]
}
