{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@nyxjs/rest",
	"version": "1.0.0",
	"description": "Rest package for Nyx.js",
	"type": "commonjs",
	"readme": "README.md",
	"main": "./dist/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"license": "AGPL-3.0-only",
	"author": {
		"name": "3tatsu",
		"url": "https://github.com/3tatsu"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/3tatsu/nyx.js.git"
	},
	"scripts": {
		"build": "pnpm run clean && tsup --config tsup.config.ts",
		"biome:write": "biome format --write src",
		"clean": "rimraf dist",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage"
	},
	"files": ["dist"],
	"dependencies": {
		"@nyxjs/cache": "workspace:*",
		"@nyxjs/core": "workspace:*",
		"eventemitter3": "^5.0.1",
		"minizlib": "^3.0.1",
		"tslib": "^2.7.0",
		"undici": "^6.19.8"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@microsoft/api-extractor": "^7.47.7",
		"@swc/core": "^1.7.22",
		"@types/node": "^22.5.2",
		"@vitest/coverage-v8": "^2.0.5",
		"@vitest/ui": "^2.0.5",
		"rimraf": "^6.0.1",
		"tsup": "^8.2.4",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5"
	}
}
