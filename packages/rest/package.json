{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxjs/rest",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && rimraf .tsup",
    "clean": "rimraf dist",
    "lint": "eslint src --fix",
    "pretty": "prettier --write src"
  },
  "dependencies": {
    "@nyxjs/core": "workspace:*",
    "@nyxjs/store": "workspace:*",
    "@nyxjs/utils": "workspace:*",
    "form-data": "^4.0.0",
    "minizlib": "^3.0.1",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.9",
    "@swc/core": "^1.7.26",
    "@types/node": "^22.5.5",
    "eslint": "^9.11.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "terser": "^5.33.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}