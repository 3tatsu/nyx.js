{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@nyxjs/tests",
  "private": true,
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "nyx.js": "workspace:*",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
