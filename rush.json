{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.135.0",
  "pnpmVersion": "9.11.0",
  "gitPolicy": {},
  "repository": {
    "url": "https://github.com/3tatsu/nyx.js",
    "defaultBranch": "main"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": [],
    "preRushx": [],
    "postRushx": []
  },
  "telemetryEnabled": false,
  "projects": [
    {
      "packageName": "@nyxjs/eslint-config",
      "projectFolder": "configs/eslint",
      "tags": ["private"]
    },
    {
      "packageName": "@nyxjs/core",
      "projectFolder": "packages/core"
    },
    {
      "packageName": "@nyxjs/rest",
      "projectFolder": "packages/rest"
    },
    {
      "packageName": "@nyxjs/store",
      "projectFolder": "packages/store"
    },
    {
      "packageName": "@nyxjs/ws",
      "projectFolder": "packages/ws"
    }
  ]
}
