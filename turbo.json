{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "lint": {
      "dependsOn": [
        "^pretty",
        "^lint"
      ]
    },
    "pretty": {
      "dependsOn": [
        "^pretty"
      ]
    },
    "vt:test": {
      "dependsOn": [
        "^vt:test"
      ]
    },
    "vt:ui": {
      "dependsOn": [
        "^vt:ui"
      ]
    },
    "vt:coverage": {
      "dependsOn": [
        "^vt:coverage"
      ]
    }
  }
}