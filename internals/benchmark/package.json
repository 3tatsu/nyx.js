{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@nyxjs/benchmark",
    "description": "Benchmark for nyx.js",
    "version": "1.0.0",
    "type": "module",
    "readme": "README.md",
    "license": "AGPL-3.0-only",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "start:discordjs": "tsx watch src/discordjs.ts",
        "start:eris": "tsx watch src/eris.ts",
        "start:nyxjs": "tsx watch src/nyxjs.ts",
        "start:oceanicjs": "tsx watch src/oceanicjs.ts",
        "format": "biome format --write ."
    },
    "dependencies": {
        "discord.js": "^14.16.3",
        "eris": "^0.18.0",
        "nyx.js": "workspace:*",
        "oceanic.js": "^1.11.2"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "^22.8.4",
        "dotenv": "^16.4.5",
        "tsx": "^4.19.2",
        "typescript": "^5.6.3"
    }
}
